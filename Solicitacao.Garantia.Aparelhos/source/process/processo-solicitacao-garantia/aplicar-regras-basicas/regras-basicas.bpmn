    <bpmn:scriptTask id="ValidarDadosAparelhoScriptTask" name="Validar dados aparelho" camunda:asyncAfter="true" scriptFormat="javascript">
      <bpmn:incoming>SequenceFlow_0972ehu</bpmn:incoming>
      <bpmn:outgoing>Flow_0awnelv</bpmn:outgoing>
      <bpmn:script>const beneficioSolicitado = JSON.parse(execution.getVariable('beneficioSolicitado'));
const clientes = JSON.parse(execution.getVariable("clientes"));
const cliente = clientes.find(c =&gt; c.cpf === beneficioSolicitado.Cpf);

const aparelhoValido = compararCampos(cliente, beneficioSolicitado);

function compararCampos(mock, valor) {
  const mockAparelho = mock.infoAparelho[0];

  const dataMock = new Date(mockAparelho.dataCompra.replace(/\//g, '-'));
  const dataValor = new Date(valor.infoAparelho.DataCompra);

  return (
    mockAparelho.notaFiscal === valor.infoAparelho.NotaFiscal &amp;&amp;
    dataMock.getTime() === dataValor.getTime()
  );
}

execution.setVariable('aparelhoElegivel', aparelhoValido);</bpmn:script>
